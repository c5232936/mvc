<!DOCTYPE html>
<html>
<head>
    @*<meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>@ViewBag.Title - My ASP.NET Application</title>
        @Styles.Render("~/Content/css")
        @Scripts.Render("~/bundles/modernizr")*@
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <title>ImSS</title>
    <link rel="stylesheet" href="~/Content/LTEcss/bootstrap.min.css">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/Content/LTEcss/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="~/Content/LTEcss/ionicons.min.css">

    <!-- DataTables -->
    <link rel="stylesheet" href="~/Content/LTEcss/dataTables.bootstrap.min.css">

    <!-- Theme style -->
    <link rel="stylesheet" href="~/Content/LTEcss/AdminLTE.min.css">
    <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
    <link rel="stylesheet" href="~/Content/LTEcss/_all-skins.min.css">
    @*<link rel="stylesheet"
          href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">*@
    <script src="~/Scripts/LTEScripts/jquery.min.js"></script>
    <!-- Bootstrap 3.3.7 -->
    <script src="~/Scripts/LTEScripts/bootstrap.min.js"></script>
    <!-- FastClick -->
    <script src="~/Scripts/LTEScripts/fastclick.js"></script>
    <!-- AdminLTE App -->
    <script src="~/Scripts/LTEScripts/adminlte.min.js"></script>
    <!-- DataTables -->
    <script src="~/Scripts/LTEScripts/jquery.dataTables.min.js"></script>
    <script src="~/Scripts/LTEScripts/dataTables.bootstrap.min.js"></script>
    <!-- SlimScroll -->
    <script src="~/Scripts/LTEScripts/jquery.slimscroll.min.js"></script>
    <!--Common CSS-->
    <link href="~/Content/css/CommonCSS.css" rel="stylesheet" />
    @*<link href="~/Content/css/all.css" rel="stylesheet" />*@
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.9.0/css/all.css" rel="stylesheet" />

    <!--Validation Scripts-->
    <script src="~/Scripts/validationscripts/jquery.validate.js"></script>
    <script src="~/Scripts/validationscripts/jquery.validate.unobtrusive.js"></script>

    <!--Bootstrap4 Scripts-->
    <script type="text/javascript" src="~/Scripts/tables/jquery.js"></script>
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/css/dataTables.bootstrap.css" rel="stylesheet" />
    <link href="~/Content/css/responsive.bootstrap.css" rel="stylesheet" />
    <script type="text/javascript" src="~/Scripts/tables/dataTables.bootstrap4.js"></script>
    <link href="~/Content/LTEcss/dataTables.bootstrap4.css" rel="stylesheet" />
    <script type="text/javascript" src="~/Scripts/Bootstrapscripts/jquery.dataTables.js"></script>
    <script type="text/javascript" src="~/Scripts/Bootstrapscripts/dataTables.bootstrap4.js"></script>
</head>
<body class="hold-transition skin-blue fixed sidebar-mini">
    <div class="wrapper">
        <header class="main-header">
            <!-- Logo -->
            <a href="#" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini"><b>ImSS</b></span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg">
                    @*<b>ImSS</b>*@
                    <img src="~/Images/Imss.png" style="height:30px; width:60px" />
                </span>
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>

            </nav>
        </header>
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <!-- Sidebar user panel -->
                <!-- search form -->
                <!-- /.search form -->
                <!-- sidebar menu: : style can be found in sidebar.less -->
                <ul class="sidebar-menu" data-widget="tree">
                    <li class="header"></li>
                    <li class="active treeview menu-open">
                        <a href="#">
                            <i class="fas fa-columns"></i><span>    Main Menu Items</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="@Url.Action("EmployeeList","EmployeeList")"><i class="fas fa-user-friends"></i>    Employee</a></li>
                            <li><a href="@Url.Action("CustomerList","CustomerList")"><i class="fas fa-briefcase"></i>   Customers</a></li>
                            <li><a href="@Url.Action("POMaster","POMaster")"><i class="fas fa-file-invoice-dollar"></i>  Purchase Orders</a></li>
                            <li><a href="@Url.Action("InvoiceList","Home")"><i class="fas fa-file-invoice"></i>    Invoices</a></li>
                        </ul>
                    </li>
                    <li>
                        <a href="#">
                            <i class="far fa-chart-bar"></i> <span>Report</span>
                            <span class="pull-right-container">
                                <small class="label pull-right bg-green">new</small>
                            </span>
                        </a>
                    </li>
                </ul>
            </section>
            <!-- /.sidebar -->
        </aside>
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            @RenderBody()
        </div>
        <!-- /.content-wrapper -->
        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                <b>Version</b> 2.4.0
            </div>
            <strong>Copyright &copy; 2019-2020 <a href="#">Integra Micro Software Services</a>.</strong> All rights
            reserved.
        </footer>
    </div>

</body>
</html>
